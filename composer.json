  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/dogado-group/laravel-cookie-manager",
  "require": {
    "php": "^7.3|^8.0",
    "cbschuld/browser.php": "^1.9",
    "symfony/http-foundation": "^4.4|^5.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.14",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
